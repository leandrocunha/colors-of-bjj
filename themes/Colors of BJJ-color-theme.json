{
  "name": "Colors of BJJ",
  "semanticHighlighting": true,
  "type": "dark",
  "colors": {
    // "editor.background": "#1D1E2A"
    "editor.background": "#191A1E"
  },
  "tokenColors": [
    {
      "name": "Default",
      "scope": "source",
      "settings": {
        "foreground": "#078ad6"
      }
    },
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#60616F9C",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Imports",
      "scope": [
        "keyword.control",
        "storage.type",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.logical"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "variable.other"],
      "settings": {
        "foreground": "#b5daf1"
      }
    },
    {
      "name": "Strings Quoted",
      "scope": ["string.quoted", "string.template"],
      "settings": {
        "foreground": "#FFF074c0"
      }
    },
    {
      "name": "Variables",
      "scope": ["meta.definition.variable"],
      "settings": {
        "foreground": "#894DE6"
      }
    },
    {
      "name": "Functions",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#A477EB"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#ebac00e1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Types",
      "scope": [
        "meta.type.annotation",
        "entity.name.type.alias",
        "meta.return.type"
      ],
      "settings": {
        "foreground": "#7D85EC",
        "fontStyle": "italic"
      }
    }
  ]
}
